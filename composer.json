{
    "name": "mikebirch/sponge-users",
    "description": "SpongeUsers plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "3.10.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "SpongeUsers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpongeUsers\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
